{
  "name": "open-fi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "watch": "nodemon --watch './**/*.{ts,graphql}' -r dotenv/config --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/server/index.ts",
