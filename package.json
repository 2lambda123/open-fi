    "scrape": "node --loader ts-node/esm --experimental-specifier-resolution=node src/example/scraper.ts   "
  },
  "dependencies": {
    "@graphql-tools/merge": "8.2.1",
    "@graphql-tools/schema": "8.3.1",
    "@prisma/client": "3.4.1",
    "apollo-server-core": "3.5.0",
    "apollo-server-express": "3.5.0",
    "date-fns": "2.25.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "graphql": "16.0.1",
    "lodash": "4.17.21",
    "neat-csv": "7.0.0",
    "node-fetch": "3.0.0",
    "nodemon": "2.0.14"
  },
  "devDependencies": {
    "@types/lodash": "4.14.176",
    "@types/node": "16.11.6",
    "prisma": "3.4.1",
    "ts-node": "10.4.0",
    "tsc-watch": "4.5.0",
    "typescript": "4.4.4"
  }
