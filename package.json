{
  "name": "open-fi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "nodemon --watch './**/*.{ts,graphql}' -r dotenv/config --exec 'tsc && node dist/server/index.js'",
    "dev": "npx tsc-watch --onSuccess \"npm run watch\"",
    "scrape": "node --loader ts-node/esm --experimental-specifier-resolution=node src/example/scraper.ts   ",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "2.0.3",
    "@graphql-tools/merge": "8.2.1",
    "@graphql-tools/schema": "8.3.1",
    "@prisma/client": "^3.4.2",
    "apollo-server-core": "3.5.0",
    "apollo-server-express": "3.5.0",
    "bull": "4.1.0",
    "date-fns": "2.25.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "graphql": "16.0.1",
    "lodash": "4.17.21",
    "neat-csv": "7.0.0",
    "node-fetch": "3.0.0",
    "nodemon": "2.0.14",
    "sofa-api": "0.11.0",
    "swagger-ui-express": "4.1.6",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/typescript": "2.3.1",
    "@graphql-codegen/typescript-resolvers": "2.4.0",
    "@types/bull": "3.15.5",
    "@types/lodash": "4.14.176",
    "@types/node": "16.11.6",
    "prisma": "^3.4.2",
    "ts-node": "10.4.0",
    "tsc-watch": "4.5.0",
    "typescript": "4.4.4"
  }
}